
@model AssignMVCRestaurantCRUD.Models.RestaurantTab
@{
    ViewBag.Title = "Details";
}

<h2>Details of a single Restaurant</h2>

<table class="table" style="background-color:antiquewhite">

    <tr>
        <th>Id</th>
        <td>@Model.Id</td>
    </tr>

    <tr>
        <th>Name</th>
        <td>@Model.Name</td>
    </tr>

    <tr>
        <th>Type</th>
        <td>@Model.Type</td>
    </tr>

</table>
<a href="/Home/Index" class="btn btn-info">Back</a>
